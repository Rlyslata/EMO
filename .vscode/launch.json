    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "model.emo"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "torch.distributed.launch",
            "args": [
                "--nproc_per_node=1",
                "--nnodes=1",
                "--use_env",
                "run.py",
                "-c",
                "configs/mobile/cls_emo",
                "-m",
                "test",
                "model.name=EMO_1M",
                "trainer.data.batch_size=32",
                "model.model_kwargs.checkpoint_path=runs_emo/CLS_EMO_1M_DefaultCLS_20240905-204902/latest_ckpt.pth"
            ],
            "console": "integratedTerminal",
            "justMyCode": false, // 允许调试库代码
            "env": {
                "MASTER_ADDR": "localhost",
                "MASTER_PORT": "12355"
            }
        }
    ]
}